{
    "version": 2,
    "builds": [
    {
    "src": "project_kirti/wsgi.py",
    "use": "@vercel/python",
    "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      }
 
    ],
    "routes": [
    {
    "src": "/static/(.*)",
    "dest": "/static/$1"
    },
    {
    "src": "/(.*)",
    "dest": "project_kirti/wsgi.py"
    }
    ]
    }
